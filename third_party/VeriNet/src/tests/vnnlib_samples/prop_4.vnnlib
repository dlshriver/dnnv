; ACAS Xu property 1

(declare-const X_0 Real)
(declare-const X_1 Real)
(declare-const X_2 Real)

(declare-const Y_0 Real)
(declare-const Y_1 Real)
(declare-const Y_2 Real)

(assert (or
    (and (<= X_0 1) (>= X_0 0.5) (<= X_1 0.5) (>= X_1 0) (<= X_2 0) (>= X_2 -0.5))
    (and (<= X_0 1.5) (>= X_0 1) (<= X_1 1) (>= X_1 0.5) (<= X_2 0.5) (>= X_2 0))
))

(assert (or
    (and (<= Y_0 Y_2))
    (and (<= Y_1 Y_2))
    (and (>= Y_1 1))
))